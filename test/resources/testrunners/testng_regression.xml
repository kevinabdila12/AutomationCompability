<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Tugas Self Learning automation regression suite"
	thread-count="3" parallel="tests" verbose="3">

	<test name="Sauce Test - Chrome">
		<parameter name="browser" value="chrome" />

		<classes>
			<class name="test1.SauceTest" />
		</classes>

	</test>
	
	<test name="Sauce Test - Edge">
		<parameter name="browser" value="edge" />

		<classes>
			<class name="test1.SauceTest" />
		</classes>

	</test>
	
	<test name="Sauce Test - Firefox">
		<parameter name="browser" value="firefox" />

		<classes>
			<class name="test1.SauceTest" />
		</classes>

	</test>
	

</suite>